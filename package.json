{
  "name": "react-noisy-canvas",
  "version": "1.0.0",
  "description": "A react component which creates an html canvas animated with noise.",
  "main": "build/NoisyCanvas.js",
  "scripts": {
    "lint": "eslint src/**",
    "build": "browserify -t babelify -o build/NoisyCanvas.js src/NoisyCanvas.js",
    "build-demo": "npm run build && browserify -t babelify -d -o demo/index.js src/demo/index.js",
    "run-demo": "serve -p 3000 . & opener http://localhost:3000/demo"
  },
  "author": "K. P. MacGregor <phoenixstormcrow@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "noisify-canvas": "^1.1.0",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phoenixstormcrow/react-noisy-canvas.git"
  },
  "keywords": [
    "react",
    "html5",
    "canvas",
    "animation"
  ],
  "bugs": {
    "url": "https://github.com/phoenixstormcrow/react-noisy-canvas/issues"
  },
  "homepage": "https://github.com/phoenixstormcrow/react-noisy-canvas"
}
